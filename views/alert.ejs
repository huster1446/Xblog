<!--显示成功或错误信息-->
<div class="row">
<% if(success){ %>
<div class="alert alert-success text-center  alert-dismissible col-lg-6 col-lg-offset-3">
	<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
    <%= success %>
</div>
<% } %>
<% if(error){ %>
<div class="alert alert-danger text-center col-lg-6 col-lg-offset-3">
	<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
    <%= error %>
</div>
<% } %>
</div>